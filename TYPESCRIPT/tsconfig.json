{
    "compilerOptions": {
      "outDir": "./built",
      "allowJs": true,
      "moduleResolution": "node",
      "module": "ESNext",
      "target": "es2022",
      "types": ["node", "@wdio/globals/types", "@wdio/mocha-framework","mocha"],
      "skipLibCheck":true,
      "noEmit": true,
      "allowImportingTsExtensions": true,
      "resolveJsonModule": true,
      "isolatedModules": true,
      "strict": true,
      "noUnusedLocals": true,
      "noUnusedParameters": true,
      "noFallthroughCasesInSwitch": true
    },
    "include": ["./src/**/*","process-env.d.ts"] 
  }

